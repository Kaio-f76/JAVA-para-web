<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>CadastroUsuarioSrv</servlet-name>
        <servlet-class>controller.CadastroUsuarioSrv</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginUsuarioSrv</servlet-name>
        <servlet-class>controller.LoginUsuarioSrv</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExibirResultados</servlet-name>
        <servlet-class>controller.ExibirResultados</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutUsuarioSrv</servlet-name>
        <servlet-class>controller.LogoutUsuarioSrv</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>tabelaDadosUsuarioSrv</servlet-name>
        <servlet-class>controller.tabelaDadosUsuarioSrv</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CadastroUsuarioSrv</servlet-name>
        <url-pattern>/CadastroUsuarioSrv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginUsuarioSrv</servlet-name>
        <url-pattern>/LoginUsuarioSrv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExibirResultados</servlet-name>
        <url-pattern>/ExibirResultados</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutUsuarioSrv</servlet-name>
        <url-pattern>/LogoutUsuarioSrv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>tabelaDadosUsuarioSrv</servlet-name>
        <url-pattern>/tabelaDadosUsuarioSrv</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
